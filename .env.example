# API ключи для LLM сервисов
OPENAI_API_KEY=your_openai_api_key_here
GIGACHAT_API_KEY=your_gigachat_api_key_here

# Настройки приложения
DATA_PATH=./data
MODEL_PROVIDER=openai
MODEL_NAME=gpt-3.5-turbo
CHUNK_SIZE=1000
CHUNK_OVERLAP=200

# Настройки веб-краулинга
ENABLE_WEB_CRAWLING=true
CRAWL_MAX_PAGES=20
CRAWL_DELAY=1.0
WEB_CRAWL_TIMEOUT=30

# Настройки поиска
SEARCH_K=5

# Логирование
LOG_LEVEL=INFO